{
  "name": "bolt-chef-unified",
  "version": "1.0.0",
  "description": "AI-powered full-stack web development tool with reactive backend",
  "type": "module",
  "bin": {
    "chefshot": "./chefshot/index.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "chefshot": "node ./chefshot/index.js",
    "test-kitchen": "node ./test-kitchen/run.js",
    "deploy:cloudflare": "node ./scripts/deploy-cloudflare.js"
  },
